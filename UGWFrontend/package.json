{
  "name": "ugwfrontend",
  "version": "0.1.190",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:pwa": "ng build --prod && npm run spa-server",
    "spa-server": "http-server-spa dist/UGWFrontend index.html 8888",
    "build": "ng build",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "cypress": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && cypress open\" --success first --kill-others",
    "e2e-chrome": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && $(npm bin)/cypress run --browser chrome\" --success first --kill-others",
    "e2e-integrated": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && $(npm bin)/cypress run \" --success first --kill-others",
    "e2e": "npm run e2e-integrated"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "~8.0.0",
    "@angular/service-worker": "~8.0.0",
    "@datorama/akita": "^3.5.5",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.2",
    "tar": "^4.4.8",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@datorama/akita-ngdevtools": "^1.0.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.1.0",
    "cypress": "^3.4.0",
    "http-server-spa": "^1.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "querystringify": ">=2.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "wait-on": "^3.2.0"
  }
}
